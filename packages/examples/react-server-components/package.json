{
  "name": "parcel-react-server-components-example",
  "version": "1.0.0",
  "private": true,
  "server": "dist/server.js",
  "targets": {
    "server": {
      "source": "src/server.js",
      "context": "node",
      "outputFormat": "commonjs",
      "packageConditions": ["react-server"],
      "includeNodeModules": {
        "express": false
      }
    },
    "client": {
      "context": "browser",
      "outputFormat": "esmodule"
    }
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "express": "^4.18.2",
    "react": "18.3.0-canary-feed8f3f9-20240118",
    "react-dom": "18.3.0-canary-feed8f3f9-20240118",
    "react-server-dom-parcel": "file:/Users/devongovett/dev/react/build/oss-stable/react-server-dom-parcel",
    "rsc-html-stream": "0.0.2",
    "@parcel/macros": "^2.12.0"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": "force"
  }
}
