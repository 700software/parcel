{
  "name": "@parcel/example-react-native",
  "version": "0.0.0",
  "scripts": {
    "start:android": "parcel serve --config .parcelrc index.js --target android --host 10.6.10.149 --hmr-port 4444",
    "start:ios": "parcel serve --config .parcelrc index.js --target ios --host 10.6.10.149 --hmr-port 4444",
    "build:android": "parcel build --config .parcelrc index.js --target ios",
    "build:ios": "parcel build --config .parcelrc index.js --target android",
    "android": "react-native run-android",
    "ios": "react-native run-ios"
  },
  "dependencies": {
    "react": "18.1.0",
    "react-native": "0.70.3"
  },
  "_browserslist": "Firefox 44",
  "browserslist_jsc": "Safari 11",
  "browserslist": "Safari 8",
  "targets": {
    "android": {
      "context": "react-native-android"
    },
    "ios": {
      "context": "react-native-ios"
    },
    "default": {}
  }
}
